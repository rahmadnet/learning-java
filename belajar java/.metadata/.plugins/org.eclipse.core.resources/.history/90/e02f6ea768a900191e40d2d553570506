
public class DocumentationExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 1; i <= 9; i++)
		{
			for(int j = 1; j <= 9; j++)
			{
				if(j ==  i)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print("-");
				}
			}
			System.out.println();
		}
		System.out.println("");
		for (int i = 1; i <= 9; i++)
		{
			for(int j = 1; j <= 9; j++)
			{
				if(j+1 == i)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print("-");
				}
			}
			System.out.println();
		}
	}

}
