
public class DocumentationExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 1; i <= 9; i++)
		{
			System.out.println("-");
			for(int j = 1; j <= i; j++)
			{
				if(j ==  i)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print("-");
				}
			}
			System.out.println();
		}
	}

}
